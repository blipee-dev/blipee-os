    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.424.0",
    "next": "^14.2.32",
    "openai": "^5.16.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^9.0.1",
    "recharts": "^2.12.7",
