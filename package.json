{
  "name": "blipee-os",
  "version": "1.0.0",
  "private": true,
  "description": "The ChatGPT for Buildings - Conversational AI Building Management",
  "author": "blipee Team",
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "optimize": "node scripts/optimize-bundle.js",
    "type-check": "tsc --noEmit",
    "type-check:strict": "bash scripts/strict-typecheck.sh",
    "prebuild": "echo 'Skipping type-check for deployment'",
    "precommit": "npm run type-check:strict && npm run lint",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.mjs",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.mjs --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.mjs --coverage",
    "test:unit": "jest --testPathPattern='(spec|test)\\.(ts|tsx)$' --testPathIgnorePatterns='integration|e2e|performance|security'",
    "test:integration": "jest --testPathPattern='integration\\.test\\.(ts|tsx)$'",
    "test:api": "jest --testPathPattern='api/.*\\.test\\.(ts|tsx)$'",
    "test:retail": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.retail.js",
    "test:retail:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.retail.js --coverage",
    "test:security": "jest --testPathPattern='security.*\\.test\\.(ts|tsx)$'",
    "test:performance": "jest --testPathPattern='performance\\.test\\.(ts|tsx)$'",
    "test:e2e": "cypress run",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:e2e:edge": "cypress run --browser edge",
    "test:e2e:open": "cypress open",
    "test:a11y": "jest --testPathPattern='accessibility\\.test\\.(ts|tsx)$'",
    "test:compliance:gdpr": "jest --testPathPattern='gdpr\\.test\\.(ts|tsx)$'",
    "test:compliance:soc2": "jest --testPathPattern='soc2\\.test\\.(ts|tsx)$'",
    "test:compliance:audit": "jest --testPathPattern='audit\\.test\\.(ts|tsx)$'",
    "test:compliance:retention": "jest --testPathPattern='retention\\.test\\.(ts|tsx)$'",
    "test:visual": "percy snapshot",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:security && npm run test:e2e",
    "test:report:generate": "node scripts/generate-test-report.js",
    "test:api:report": "jest --testPathPattern='api/.*\\.test\\.(ts|tsx)$' --json --outputFile=test-results/api/results.json",
    "compliance:report": "node scripts/generate-compliance-report.js",
    "security:scan": "npm audit && snyk test",
    "analyze": "cross-env ANALYZE=true next build",
    "bundlesize": "bundlesize",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags",
    "changelog": "node scripts/update-changelog.js",
    "backup:database": "tsx scripts/backup/supabase-backup.ts backup",
    "backup:restore": "tsx scripts/backup/supabase-backup.ts restore",
    "backup:clean": "tsx scripts/backup/supabase-backup.ts clean",
    "release": "npm run build && npm run lint && npm run type-check && npm run version:minor",
    "test:apis": "tsx scripts/test-api-connections.ts",
    "populate-data": "tsx scripts/populate-sustainability-data.ts",
    "extract-reports": "tsx scripts/extract-report-data.ts",
    "baseline:record": "tsx scripts/performance-baseline.ts record",
    "baseline:compare": "tsx scripts/performance-baseline.ts compare",
    "test:csrf": "tsx scripts/test-csrf-protection.ts",
    "test:xss": "tsx scripts/test-xss-protection.ts",
    "test:session-security": "tsx scripts/test-session-security.ts",
    "test:security:all": "tsx scripts/security-test-suite.ts"
  },
